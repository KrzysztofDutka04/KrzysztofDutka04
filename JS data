<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	
</head>
<body>
	<div></div>
	<script>
		const div = document.querySelector('div');
		const data = new Date();
		const rok = data.getFullYear();
		const miesiac = data.getMonth() + 1;
		const dzien = data.getDate();
		div.textContent = `${rok} / ${miesiac} / ${dzien}`;
	</script>
</body>
</html>
